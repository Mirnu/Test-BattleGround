{
	"$schema": "https://biomejs.dev/schemas/1.5.2/schema.json",
	"vcs": {
		"enabled": true,
		"clientKind": "git",
		"useIgnoreFile": true
	},
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true,
			"complexity": {
				"noBannedTypes": "off"
			},
			"style": {
				"noDefaultExport": "error",
				"useImportType": "off"
			},
			"suspicious": {
				"noShadowRestrictedNames": "off",
				"noEmptyInterface": "off"
			}
		}
	}
}
